<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mapsFragment">

    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.myapplication.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_mapsFragment_to_problemsFragment"
            app:destination="@id/problemsFragment" />
    </fragment>
    <fragment
        android:id="@+id/problemsFragment"
        android:name="com.example.myapplication.ProblemsFragment"
        android:label="fragment_problems"
        tools:layout="@layout/fragment_problems" />
</navigation>